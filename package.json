{
	"name": "sb-reg-svelte5",
	"version": "1.5.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/westborn/SB-Reg-svelte5"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"netlify-build": "prisma generate && pnpm run build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"db:migrate": "prisma migrate dev",
		"db:reset": "prisma migrate reset --force",
		"db:seed": "tsx src/scripts/seed.ts",
		"studio": "prisma studio",
		"upgrade-deps": "ncu -uis --format group --install always",
		"bump:patch": "npm --no-git-tag-version version patch",
		"bump:minor": "npm --no-git-tag-version version minor",
		"bump:major": "npm --no-git-tag-version version major"
	},
	"devDependencies": {
		"@exodus/schemasafe": "^1.3.0",
		"@faker-js/faker": "^9.4.0",
